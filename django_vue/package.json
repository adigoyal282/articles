{
  "name": "django_vue",
  "version": "1.0.0",
  "description": "Make DJango and Vue play well together",
  "main": "webpack.config.js",
  "scripts": {
    "build": "webpack --progress --profile --debug --config webpack.config.js",
    "watch": "webpack-dev-server --config webpack.watch.config.js -http--port:8080",
    "release": "webpack --progress --config webpack.release.config.js",
    "test": "./node_modules/karma/bin/karma start karma.config.js",
    "test:coverage": "./node_modules/karma/bin/karma start karma.coverage.config.js && open ./project/web/app/test/coverage/project/web/app/components/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/priyankcommits/articles.git"
  },
  "keywords": [
    "vue",
    "django",
    "javascript",
    "webpack"
  ],
  "author": "pulumati.priyank@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/priyankcommits/articles/issues"
  },
  "homepage": "https://github.com/priyankcommits/articles#readme",
  "dependencies": {
    "vue": "2.5.3"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.11",
    "chai": "4.1.2",
    "css-loader": "^0.28.9",
    "file-loader": "2.0.0",
    "istanbul": "0.4.5",
    "istanbul-instrumenter-loader": "3.0.0",
    "karma": "3.1.1",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.2",
    "karma-coverage-istanbul-reporter": "1.4.1",
    "karma-istanbul": "0.0.2",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-sinon": "1.0.5",
    "karma-sinon-chai": "1.3.3",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "3.0.0",
    "mocha": "5.0.0",
    "sinon": "4.2.2",
    "sinon-chai": "2.14.0",
    "vue-loader": "12.2.2",
    "vue-template-compiler": "2.5.3",
    "webpack": "2.7.0",
    "webpack-bundle-tracker": "0.3.0",
    "webpack-cli": "3.1.2",
